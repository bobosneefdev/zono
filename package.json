{
    "name": "@bobosneefdev/zono",
    "version": "4.0.0",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./shared": {
            "types": "./dist/shared.d.ts",
            "import": "./dist/shared.mjs",
            "require": "./dist/shared.js"
        },
        "./client": {
            "types": "./dist/client.d.ts",
            "import": "./dist/client.mjs",
            "require": "./dist/client.js"
        },
        "./server": {
            "types": "./dist/server.d.ts",
            "import": "./dist/server.mjs",
            "require": "./dist/server.js"
        }
    },
    "scripts": {
        "prepublishOnly": "npm run build",
        "build": "tsc --noEmit && tsup",
        "test": "bun test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bobosneefdev/zono.git"
    },
    "keywords": [
        "zod",
        "hono",
        "zono"
    ],
    "author": "@bobosneefdev",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bobosneefdev/zono/issues"
    },
    "homepage": "https://github.com/bobosneefdev/zono#readme",
    "description": "Easily create type-safe APIs, clients, and servers with Zod and Hono.",
    "dependencies": {
        "@socket.io/bun-engine": "^0.0.2",
        "@types/bun": "^1.2.22",
        "axios": "^1.12.2",
        "hono": "^4.9.7",
        "socket.io": "^4.8.1",
        "zod": "^4.1.9",
        "zod-openapi": "^5.4.1"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "socket.io-client": "^4.8.1",
        "tsup": "^8.5.0",
        "typescript": "^5.9.2"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org"
    },
    "files": [
        "dist",
        "LICENSE"
    ]
}
